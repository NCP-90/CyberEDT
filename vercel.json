{
    "functions": {
        "api/scan.js": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/scan",
            "destination": "/api/scan.js"
        },
        {
            "source": "/tools/phishing-analyzer/assets/:path*",
            "destination": "/tools/phishing-analyzer/dist/assets/:path*"
        },
        {
            "source": "/tools/phishing-analyzer",
            "destination": "/tools/phishing-analyzer/dist/index.html"
        },
        {
            "source": "/tools/phishing-analyzer/",
            "destination": "/tools/phishing-analyzer/dist/index.html"
        }
    ]
}